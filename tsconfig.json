{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    // "jsx": "react", // From w2w
    // "baseUrl": "src",
    // "noImplicitAny": false,
    // "checkJs": false,
    // "noFallthroughCasesInSwitch": true // from w2w ends
    "jsx": "preserve",
    "baseUrl": "src",
    "noImplicitAny": false,
    "checkJs": false
  },
  "include": [
    "next-env.d.ts",
    "src/config/custom.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/segments/ViewChannels.jsx",
    "src/components/SearchFilter.jsx",
    "src/components/chat/w2wChat/indexeddb.js", 
    "src/contexts/VideoCallContext.js"  
  ]
}
